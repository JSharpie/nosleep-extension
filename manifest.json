{
  "manifest_version": 2,

  "name": "r/NoSleep latest links",
  "description": "This extension shows all the latest links from reddit.com/r/nosleep",
  "version": "1.0",
  "icons":{
    "48":"icon.png"
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click Here!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
